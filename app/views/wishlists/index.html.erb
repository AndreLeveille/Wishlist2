<%= button_to "Log Out", destroy_user_session_path, method: :delete %>
<h2>Welcome <%= current_user.first_name %></h2>

<h2> My Wish Lists </h2>
<%= link_to 'Create new wish list', new_wishlist_path %>
<ul>
    <% @wishlists.each do |list| %>
        <li>
           
            <%= list.name %> <%= link_to 'Edit this list', edit_wishlist_path(list) %> 
            <%= link_to 'Delete this wish list', wishlist_path(list), method: :delete%>
        </li>
    <% end %>
</ul>